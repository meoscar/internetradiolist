	<script>
		// Start Ready
		$(document).ready(function() {
			// Icon Click Focus
			$('div.icon').click(function() {
				$('#main_search').focus();
			});

			// Live Search
			// On Search Submit and Get Results
			function search() {

				var search_type = $("#search_type").val();
				var query_value = $('#main_search').val();
				$('#main-search-string').text(query_value);

				if (query_value !== '') {


					if (search_type == 1) {


						$.ajax({
							type: "POST",
							url: "searchby-station/searchbystation.php",
							data: {
								query: query_value
							},
							cache: false,
							success: function(html) {
								$("#main_results").html(html);
							}
						});

					} else if (search_type == 3) {
						$.ajax({
							type: "POST",
							url: "searchby-podcast/searchbypodcast.php",
							data: {
								query: query_value
							},
							cache: false,
							success: function(html) {
								$("#main_results").html(html);
							}
						});
					} else {

						$.ajax({
							type: "POST",
							url: "searchby-song/searchbysong.php",
							data: {
								query: query_value
							},
							cache: false,
							success: function(html) {
								$("#main_results").html(html);
							}
						});

					}


				}
				return false;
			}


			$('#main_search').keyup(function(e) {

				// Set Timeout
				clearTimeout($.data(this, 'timer'));

				// Set Search String
				var search_string = $(this).val();

				// Do Search
				if (search_string == '') {
					$("#main_results").fadeOut();
					$('#main_results-text').fadeOut();
				} else {
					$("#main_results").fadeIn();
					$('#main_results-text').fadeIn();
					$(this).data('timer', setTimeout(search, 100));
				};
			});

			$('#search_type').change(function(e) {
				$("#main_search").val('');
				$("#main_results").html('');
			});

		});
	</script>
